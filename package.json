{
  "name": "delegate-mcp",
  "version": "0.1.0",
  "description": "Model Context Protocol implementation with context preparation and Gemini model delegation",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write 'src/**/*.ts'",
    "debug:json": "cross-env LOG_LEVEL=debug ts-node src/utils/debug-test.ts",
    "debug:json:build": "cross-env LOG_LEVEL=debug node dist/utils/debug-test.js",
    "debug:simple": "ts-node src/utils/simple-debug.ts",
    "debug:minimal": "ts-node src/utils/minimal-server.ts",
    "debug:direct": "ts-node src/utils/direct-transport.ts"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cross-env": "^7.0.3",
    "dotenv": "^16.4.1",
    "glob": "^10.3.10",
    "octokit": "^3.1.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
    "eslint": "^8.56.0",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "bin": {
    "delegate-mcp": "./dist/index.js"
  }
}
